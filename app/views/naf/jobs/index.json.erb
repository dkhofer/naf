<%
  rows = @jobs.each do |job|
    job[11] = link_to image_tag('papertrail_job.png', :class => 'action', :title => 'View job log in Papertrail'), job[11], { :target => '_blank', :class => "papertrail", :id => job[0] }
    job[11] << "&nbsp;&nbsp;&nbsp;".html_safe
    unless job[9] == "Canceled"
      job[11] << (link_to image_tag('terminate.png', :class => 'action', :title => 'Terminate Job'), "#", {:class => "terminate", :id => job[0]})
    end
  end
%>
<%= raw rows %>
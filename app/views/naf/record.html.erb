<div id="record">

<%= generate_back_link %>
<%= generate_edit_link %>

<h3 id="record_name"><%= model_name %> </h3>
<p id="notice"><%= notice %></p>

<table id="record">

 <tr>
   <th>Attribute</th>
   <th>Value </th>
 </tr>

<tbody>
 <% @attributes.each do |a| %>
   <tr>
     <td scope="row"><%= make_header(a.to_s) %></td>
     <td> <%= @record.send(a) %></td>
   </tr>
 <% end %>
</tbody>
</table>
<%= generate_affinity_tabs_link %>
<%= generate_destroy_link %>
</div>
